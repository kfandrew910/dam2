 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
        Alumno a;
        Grupo g;
        
        a = Alumno.builder().nia("001").nota(0).nombre("1").build();
        
        System.out.println(a);
        
        g = Grupo.builder().nombre("DAM2").tutor("Mario").alumno(a);
    }
}
